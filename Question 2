#include <msp430.h>

int main(void)
{
    WDTCTL = WDTPW | WDTHOLD;   // stop watchdog timer

    int a = 0b10000001; //binary arithmatic
    int b = 0b10000001; // binary arithmatic
    int result;

    result = sum(a,b);

    while(1);
}

int sum(int a,int b)
{
    int result = a+b;
    return result;
}
